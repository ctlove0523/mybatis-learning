<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="io.github.ctlove0523.mybatis.started.mapper.CountryMapper">
    <resultMap id="countryResultMap" type="Country">
        <id property="countryId" column="country_id" />
        <result property="country" column="country"/>
        <result property="lastUpdate" column="last_update"/>
    </resultMap>

    <select id="selectCountry" parameterType="int" resultMap="countryResultMap">
        select * from country where country_id = #{id}
    </select>
</mapper>